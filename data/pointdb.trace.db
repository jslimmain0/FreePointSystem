                            CONSTRAINT FK_EARN_POINT  FOREIGN KEY (POINT_KEY) REFERENCES POINT(POINT_KEY),
                            CONSTRAINT FK_EARN_WALLET FOREIGN KEY (WALLET_KEY) REFERENCES POINT_WALLET(WALLET_KEY)
);

